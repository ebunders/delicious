        {
            "name": "Tahoe boemboe Bali",
            "id":"0",
            "ref": "beb-vuyk:361",
            "persons": 4,
            "ingredients":[
                {
                    "section": "main",
                    "contents": [
                        {"amount": 1 , "ingredient": "to"},
                        {"amount": 0.16 , "ingredient": "sa"},
                        {"amount": 2 , "unit": "dl", "ingredient": "wa"},
                        {"amount": 1 , "unit": "dl", "ingredient": "zo"}
                    ]
                },
                {
                    "section": "kruiden pasta",
                    "contents": [
                        {"amount": 3 , "ingredient": "gui", "unit": "el"},
                        {"amount": 2 , "unit": "tn", "ingredient": "kno", "modifier": "gesnipperde"},
                        {"amount": 2 , "unit": "tl", "ingredient": "saoe"},
                        {"amount": 5 ,  "ingredient": "kn", "modifier": "gepofte"},
